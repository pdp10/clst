# /etc/exports: the access control list for filesystems which may be exported
#               to NFS clients.  See exports(5).
#
# Example for NFSv2 and NFSv3:
# /srv/homes       hostname1(rw,sync,no_subtree_check) hostname2(ro,sync,no_subtree_check)
#
# Example for NFSv4:
# /srv/nfs4        gss/krb5i(rw,sync,fsid=0,crossmnt,no_subtree_check)
# /srv/nfs4/homes  gss/krb5i(rw,sync,no_subtree_check)
#
/export iah526(rw,fsid=0,insecure,no_root_squash,async,no_subtree_check) iah527(rw,fsid=0,insecure,no_root_squash,async,no_subtree_check) iah528(rw,fsid=0,insecure,no_root_squash,async,no_subtree_check) iah529(rw,fsid=0,insecure,no_root_squash,async,no_subtree_check) iah530(rw,fsid=0,insecure,no_root_squash,async,no_subtree_check) iah531(rw,fsid=0,insecure,no_root_squash,async,no_subtree_check) iah-huygens(rw,fsid=0,insecure,no_root_squash,async,no_subtree_check)
/export/home iah526(rw,nohide,insecure,no_root_squash,async,no_subtree_check) iah527(rw,nohide,insecure,no_root_squash,async,no_subtree_check) iah528(rw,nohide,insecure,no_root_squash,async,no_subtree_check) iah529(rw,nohide,insecure,no_root_squash,async,no_subtree_check) iah530(rw,nohide,insecure,no_root_squash,async,no_subtree_check) iah531(rw,nohide,insecure,no_root_squash,async,no_subtree_check) iah-huygens(rw,nohide,insecure,no_root_squash,async,no_subtree_check)
/export/home/modellers iah526(rw,nohide,insecure,no_root_squash,async,no_subtree_check) iah527(rw,nohide,insecure,no_root_squash,async,no_subtree_check) iah528(rw,nohide,insecure,no_root_squash,async,no_subtree_check) iah529(rw,nohide,insecure,no_root_squash,async,no_subtree_check) iah-huygens(rw,nohide,insecure,no_root_squash,async,no_subtree_check)
/export/opt iah526(ro,nohide,insecure,async,no_subtree_check) iah527(ro,nohide,insecure,async,no_subtree_check) iah528(ro,nohide,insecure,async,no_subtree_check) iah529(ro,nohide,insecure,async,no_subtree_check) iah530(ro,nohide,insecure,async,no_subtree_check) iah531(ro,nohide,insecure,async,no_subtree_check) iah-huygens(ro,nohide,insecure,async,no_subtree_check)


